{
  "name": "JavaFX Codespaces",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [5901],
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "redhat.java"
      ]
    }
  },
  "postCreateCommand": "vncserver :1 -geometry 1280x800 -depth 24"
}
